{
  "name": "hemera-zipkin",
  "version": "1.0.7",
  "description": "This is a plugin to use Zipkin with Hemera",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:hemerajs/hemera.git",
    "type": "git"
  },
  "scripts": {
    "test": "mocha -t 5000 --recursive",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec -t 5000 --recursive",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report text-lcov -- -R spec | node ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint --ext .js lib test"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Dustin Deus (https://github.com/StarpTech)",
  "license": "MIT",
  "dependencies": {
    "hoek": "4.1.x",
    "wreck": "10.0.x"
  },
  "devDependencies": {
    "code": "4.0.x",
    "coveralls": "2.11.x",
    "eslint": "3.19.x",
    "eslint-config-standard": "10.2.x",
    "eslint-plugin-import": "2.2.x",
    "eslint-plugin-node": "4.2.x",
    "eslint-plugin-promise": "3.5.x",
    "eslint-plugin-standard": "3.0.x",
    "hemera-testsuite": "1.1.x",
    "istanbul": "0.4.x",
    "mocha": "3.3.x",
    "mocha-lcov-reporter": "1.3.x",
    "nyc": "10.3.x"
  }
}
